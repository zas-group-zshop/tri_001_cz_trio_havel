                    <div class="pageDesc modal-desc">
                        <div class="modal-title"><asp:Literal ID="lh1" runat="server" Text="<%$ Resources: Resource, PageLoginTitle %>"></asp:Literal></div>
                        <asp:Panel ID="pDescription" runat="server" cssClass="cartPopupDesc">
