                            <zas:ZasDropDownList ID="eshop_stat" data-attr-stat runat="server" CssClass="form-change-country form-control" DataTextField="nazev" DataValueField="stat" Source="z_statycis_items/z_statycis_item" />
                        </div>
                    </div>
			        <div class="modal-buttons">
                        <asp:LinkButton runat="server" CssClass="button btn btn-sm btn-default" Text='OK' onclick="SelectState_Click" />
			        </div>
                </asp:Panel>
